Grammar: StepX
N: X
T: a
X -> read <<< X a
X -> loop <<< X
//

Grammar: DelL
N: L
T: e
L -> nil <<< e
//

Grammar: DelX
N: X
T: e
X -> nil <<< e
//

Grammar: LoopX
N: X
S: X
X -> loop <<< X
//

Grammar: Local
N: X
N: R
N: L
T: a
S: R
R -> lcl <<< R a
R -> id  <<< X
X -> id  <<< L
L -> lcl <<< L a
//

Product: NW
StepX >< StepX + Local >< LoopX - LoopX * 2 + DelL >< DelX
//

